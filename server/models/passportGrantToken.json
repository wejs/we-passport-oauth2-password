{
  "attributes": {
    "access_token": {
      "type": "STRING",
      "unique": true,
      "allowNull": false
    },
    "refresh_token": {
      "type": "STRING",
      "unique": true,
      "allowNull": false
    },
    "token_type": {
      "type": "STRING"
    },
    "scopes": {
      "type": "TEXT"
    },
    "expireDate": {
      "type": "INTEGER",
      "allowNull": false
    },
    "isValid": {
      "type": "BOOLEAN",
      "defaultValue": true
    }
  },
  "associations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "allowNull": false
    }
  },
  "hooks": {
    "beforeValidate": ["createPassportGrantToken"]
  }
}

