{
  "attributes": {
    "token": {
      "type": "STRING",
      "unique": true,
      "defaultValue": true
    },
    "scopes": {
      "type": "TEXT"
    },
    "expireDate": {
      "type": "DATE",
      "allowNull": false
    },
    "isValid": {
      "type": "BOOLEAN",
      "defaultValue": true
    }
  },
  "associations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "allowNull": false
    }
  },
  "hooks": {
    "beforeCreate": ["createPassportGrantToken"]
  }
}

